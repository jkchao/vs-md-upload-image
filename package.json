{
  "name": "vs-md-upload-image",
  "displayName": "vs-md-upload-image",
  "description": "upload image in vscode when write .md",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true,
    "commands": [
      {
        "command": "QN.select.file",
        "title": "Upload image: select file to upload",
        "when": "editorTextFocus && editorLangId == 'markdown'"
      }
    ],
    "configuration": {
      "title": "qiniu config",
      "properties": {
        "qiniu.ak": {
          "type": "string",
          "description": "七牛 AK"
        },
        "qiniu.sk": {
          "type": "string",
          "description": "七牛 SK"
        },
        "qiniu.bucket": {
          "type": "string",
          "description": "七牛 bucket"
        },
        "qiniu.prefix": {
          "type": "string",
          "description": "上传文件时的路径前缀"
        },
        "qiniu.domain": {
          "type": "string",
          "description": "上传成功后的图片 url 的 domain"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,ts,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "dependencies": {
    "markdown-it-emoji": "^1.4.0",
    "qiniu": "^7.2.1",
    "tinify": "^1.6.0-beta.2"
  }
}
